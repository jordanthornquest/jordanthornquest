{
  "name": "jordanthornquest",
  "version": "1.0.0",
  "description": "Personal website built with Eleventy.",
  "devDependencies": {
    "@11ty/eleventy": "^0.11.1",
    "autoprefixer": "^10.2.5",
    "del-cli": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.8",
    "postcss-cli": "^8.3.1",
    "postcss-import": "^14.0.0",
    "tailwindcss": "^2.0.3"
  },
  "scripts": {
    "build": "npm-run-all clean build:site build:styles",
    "build:site": "ELEVENTY_ENV=development npx @11ty/eleventy",
    "clean": "npx del dist/",
    "start": "npm-run-all clean --parallel watch:site watch:styles",
    "watch:site": "ELEVENTY_ENV=production npx @11ty/eleventy --serve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jordanthornquest/jordanthornquest.git"
  },
