{
  "name": "jordanthornquest",
  "version": "1.0.0",
  "description": "Personal website built with Eleventy.",
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "autoprefixer": "^10.2.5",
    "del-cli": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.10",
    "postcss-cli": "^8.3.1",
    "postcss-import": "^14.0.0",
    "tailwindcss": "^2.1.0"
  },
  "scripts": {
    "build": "npm-run-all clean build:site build:styles",
    "build:site": "ELEVENTY_ENV=production npx @11ty/eleventy",
    "build:styles": "NODE_ENV=production postcss ./styles/styles.css -d ./dist/css/ --config ./postcss.config.js",
    "clean": "npx del dist/",
    "start": "npm-run-all clean --parallel watch:styles watch:site",
    "watch:site": "ELEVENTY_ENV=development npx @11ty/eleventy --serve",
    "watch:styles": "postcss ./styles/styles.css -d ./.cache/css/ --config ./postcss.config.js -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jordanthornquest/jordanthornquest.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jordanthornquest/jordanthornquest/issues"
  },
  "homepage": "https://github.com/jordanthornquest/jordanthornquest#readme"
}
